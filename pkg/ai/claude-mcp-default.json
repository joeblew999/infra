{
  "servers": [
    {
      "name": "github",
      "version": "2025.1.24",
      "repo": "modelcontextprotocol/server-github",
      "type": "stdio",
      "command": "bun",
      "args": ["x", "-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    {
      "name": "filesystem",
      "version": "2025.1.24",
      "repo": "modelcontextprotocol/server-filesystem",
      "type": "stdio",
      "command": "bun",
      "args": ["x", "-y", "@modelcontextprotocol/server-filesystem"],
      "env": {
        "ALLOWED_PATHS": "${HOME}"
      }
    },
    {
      "name": "fetch",
      "version": "2025.1.24",
      "repo": "modelcontextprotocol/server-fetch",
      "type": "stdio",
      "command": "bun",
      "args": ["x", "-y", "@modelcontextprotocol/server-fetch"]
    }
  ]
}