{
  "binaries": [
    {
      "name": "pocketbase",
      "version": "0.21.0",
      "source": "go-build",
      "path": "./cmd/pocketbase"
    }
  ],
  "process": {
    "command": "${dep.pocketbase}",
    "args": [],
    "env": {
      "POCKETBASE_DIR": "${data}/pocketbase",
      "CORE_POCKETBASE_SMTP_HOST": "${env.CORE_POCKETBASE_SMTP_HOST}",
      "CORE_POCKETBASE_SMTP_PORT": "${env.CORE_POCKETBASE_SMTP_PORT}",
      "CORE_POCKETBASE_SMTP_USERNAME": "${env.CORE_POCKETBASE_SMTP_USERNAME}",
      "CORE_POCKETBASE_SMTP_PASSWORD": "${env.CORE_POCKETBASE_SMTP_PASSWORD}",
      "CORE_POCKETBASE_SMTP_FROM": "${env.CORE_POCKETBASE_SMTP_FROM}",
      "CORE_POCKETBASE_SMTP_TLS": "${env.CORE_POCKETBASE_SMTP_TLS}",
      "CORE_POCKETBASE_APP_URL": "${env.CORE_POCKETBASE_APP_URL}",
      "CORE_POCKETBASE_ADMIN_EMAIL": "${env.CORE_POCKETBASE_ADMIN_EMAIL}",
      "CORE_POCKETBASE_ADMIN_PASSWORD": "${env.CORE_POCKETBASE_ADMIN_PASSWORD}",
      "CORE_POCKETBASE_GOOGLE_CLIENT_ID": "${env.CORE_POCKETBASE_GOOGLE_CLIENT_ID}",
      "CORE_POCKETBASE_GOOGLE_CLIENT_SECRET": "${env.CORE_POCKETBASE_GOOGLE_CLIENT_SECRET}",
      "CORE_POCKETBASE_GITHUB_CLIENT_ID": "${env.CORE_POCKETBASE_GITHUB_CLIENT_ID}",
      "CORE_POCKETBASE_GITHUB_CLIENT_SECRET": "${env.CORE_POCKETBASE_GITHUB_CLIENT_SECRET}",
      "CORE_POCKETBASE_MICROSOFT_CLIENT_ID": "${env.CORE_POCKETBASE_MICROSOFT_CLIENT_ID}",
      "CORE_POCKETBASE_MICROSOFT_CLIENT_SECRET": "${env.CORE_POCKETBASE_MICROSOFT_CLIENT_SECRET}",
      "CORE_POCKETBASE_APPLE_CLIENT_ID": "${env.CORE_POCKETBASE_APPLE_CLIENT_ID}",
      "CORE_POCKETBASE_APPLE_CLIENT_SECRET": "${env.CORE_POCKETBASE_APPLE_CLIENT_SECRET}"
    },
    "compose": {
      "availability": {
        "restart": "always"
      },
      "depends_on": {
        "nats": {
          "condition": "process_healthy"
        }
      },
      "readiness_probe": {
        "http_get": {
          "url": "http://127.0.0.1:8090/api/health"
        },
        "initial_delay_seconds": 5,
        "period_seconds": 5,
        "timeout_seconds": 3,
        "success_threshold": 1,
        "failure_threshold": 5
      }
    }
  },
  "ports": {
    "primary": {
      "port": 8090,
      "protocol": "http"
    }
  },
  "scalable": false,
  "scale_strategy": "infra"
}
